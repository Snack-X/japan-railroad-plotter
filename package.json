{
  "name": "japan-railroad-plotter",
  "version": "1.2.0",
  "description": "Japan Railroad Plotter",
  "author": "Snack <korsnack@korsnack.kr>",
  "license": "MIT",
  "scripts": {
    "preview-py2": "python -m SimpleHTTPServer",
    "preview-py3": "python3 -m http.server",
    "build": "npm run build-js && npm run build-css",
    "build-js": "webpack --mode production",
    "build-js:dev": "webpack --mode development",
    "build-css": "node-sass assets/scss/app.scss --output-style compressed > assets/app.css",
    "build-css:dev": "node-sass assets/scss/app.scss --output-style expanded > assets/app.css"
  },
  "dependencies": {
    "lz-string": "^1.4.4",
    "mapshaper": "^0.4.64",
    "topojson": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "node-sass": "^4.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  }
}
